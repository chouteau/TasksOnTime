<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <Version>1.0.14.0</Version>
		<RestoreProjectStyle>PackageReference</RestoreProjectStyle>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ariane5.Azure" Version="5.2.18" />
    <PackageReference Include="TasksOnTimeCore" Version="1.2.18.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DistributedTasksOnTime.Shared\DistributedTasksOnTime.Shared.csproj">
		<PrivateAssets>all</PrivateAssets>
    </ProjectReference>
  </ItemGroup>
	
	<ItemGroup>
		<Content Include="$(OutputPath)\DistributedTasksOnTime.Shared.dll">
			<BuildAction>None</BuildAction>
			<PackagePath>lib/net6.0</PackagePath>
		</Content>
	</ItemGroup>

</Project>
